            },
            "nullable": true
          },
          "procrastinationFactor": {
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "color": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "subTasks": {
        "type": "object",
