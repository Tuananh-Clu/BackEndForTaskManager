          },
          "description": {
            "type": "string",
            "nullable": true
          },
          "status": {
            "minLength": 1,
            "type": "string"
          },
          "priority": {
            "minLength": 1,
            "type": "string"
          },
          "tags": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          },
          "estimateMin": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "actualMin": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "createdAt": {
            "minLength": 1,
            "type": "string"
          },
          "startedAt": {
            "type": "string",
            "nullable": true
          },
          "dueAt": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          },
          "completedAt": {
            "type": "string",
            "nullable": true
          },
          "repeat": {
            "type": "string",
